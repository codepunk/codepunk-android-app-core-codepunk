<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="@string/settings_developer_options"
    >

    <ListPreference
        android:defaultValue="local"
        android:enabled="true"
        android:icon="@drawable/ic_preference_database"
        android:key="@string/pref_key_dev_options_auth_environment"
        android:order="100"
        android:selectable="true"
        android:title="@string/settings_remote_environment"
        />

    <!--suppress AndroidElementNotAllowed -->
    <com.codepunk.doofenschmirtz.preference.TwoTargetSwitchPreference
        android:defaultValue="false"
        android:enabled="true"
        android:icon="@drawable/ic_preference_link"
        android:key="@string/pref_key_dev_options_override_base_url"
        android:order="200"
        android:persistent="false"
        android:selectable="true"
        android:title="@string/settings_dev_options_override_base_url"
        />

</PreferenceScreen>
